{% extends 'oragePage.html.twig' %}

{% block title %}Contact{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href={{ asset("styles/contact.css") }}>
{% endblock %}

{% block navbody %}

{# Banner Section#}
<div class ="main-content">
{% set banner_title = "Contactez-nous"%}
{% set banner_description = "<p>Questions ou suggestions, nous sommes disponible toute la semaine. </p>"%}
{% include "component/banner.html.twig"%}
{% include 'component/flashRenderer.html.twig'%}

    <div class ="etiquette-container">
<div class="etiquette">
    <a class ="reset-a" href={{ url("contact.faq") }}>
        <img class ="etiquette-icon" src={{ asset("images/faq.svg") }} alt="icon">
        <h2>FAQ</h2>
        <p>Vos questions ont peut-être déjà leurs réponses !</p>
    </a>
</div>
<div class="etiquette">
    <a class ="reset-a" href={{ url("contact.mail") }}>
        <img class ="etiquette-icon" src={{ asset("images/envelope.svg") }} alt="icon">
        <h2>Par mail</h2>
        <p>Ecrivez nous à l'adresse mail: <br>contact@asso-orage.fr</p>
    </a>
</div>

</div>
</div>


{% endblock %}
